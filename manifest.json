{
  "manifest_version": 3,
  "name": "Protheus Manager - Multi Agent",
  "version": "1.0",
  "description": "Gerencie m√∫ltiplos agentes Protheus.",
  "permissions": ["storage", "windows","tabs","activeTab","http://*/*",
        "https://*/*",
        "declarativeContent"],
  "host_permissions": ["<all_urls>"],
  "action": { "default_popup": "popup.html" },
  "icons": { "16": "icons/16.png", "48": "icons/48.png", "128": "icons/72.png" },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}